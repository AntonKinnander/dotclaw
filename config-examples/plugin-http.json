{
  "name": "github_search",
  "description": "Search GitHub repositories",
  "type": "http",
  "method": "GET",
  "url": "https://api.github.com/search/repositories",
  "headers": {
    "Authorization": "Bearer ${GITHUB_TOKEN}"
  },
  "query_params": {
    "q": "{{query}}",
    "per_page": "5"
  },
  "input": {
    "query": "string"
  },
  "required": ["query"]
}
